<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href=""?>

<plugin>
    <name>openXReports</name>
    <displayName>Reports Plugin</displayName>
    <creationDate>2025-08-18</creationDate>
    <author>Revive Adserver</author>
    <authorEmail>revive@revive-adserver.com</authorEmail>
    <authorUrl>http://www.revive-adserver.com</authorUrl>
    <license>LICENSE.txt</license>
    <description>Plugin that provides standard reports.</description>
    <version>1.6.10</version>
    <oxversion>3.2.0-beta-rc3</oxversion>
    <type>package</type>

    <install>

        <files>
            <file path="{PLUGINPATH}">openXReports.readme.txt</file>
        </files>

        <!-- list the groups that are contained in this plugin -->
        <!-- in the order they should be installed -->
        <contents>
            <group name="oxReportsStandard">0</group>
            <group name="oxReportsAdmin">1</group>
        </contents>

    </install>

</plugin>
