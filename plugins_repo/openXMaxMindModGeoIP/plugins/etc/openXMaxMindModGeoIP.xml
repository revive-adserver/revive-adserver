<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href=""?>

<plugin>
    <name>openXMaxMindModGeoIP</name>
    <creationDate>2014-04-08</creationDate>
    <author>Revive Adserver</author>
    <authorEmail>revive@revive-adserver.com</authorEmail>
    <authorUrl>http://www.revive-adserver.com</authorUrl>
    <license>LICENSE.txt</license>
    <description>Plugin that provides MaxMind GeoIP Geotargeting via mod_geoip.</description>
    <version>1.0.4</version>
    <oxversion>3.2.0-beta-rc3</oxversion>
    <type>package</type>

    <install>

        <files>
            <file path="{PLUGINPATH}">openXMaxMindModGeoIP.readme.txt</file>
        </files>

        <!-- list the groups that are contained in this plugin -->
        <!-- in the order they should be installed -->
        <contents>
            <group name="oxMaxMindModGeoIP">0</group>
        </contents>

    </install>

</plugin>
